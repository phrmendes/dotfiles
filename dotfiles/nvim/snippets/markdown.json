{
  "Block math": {
    "prefix": "bm",
    "body": ["$$", "${1:equation}", "$$", "$0"],
    "description": "Add block math"
  },
  "Inline math": {
    "prefix": "im",
    "body": ["$${1:equation}$$0"],
    "description": "Add inline math"
  },
  "Recipe": {
    "prefix": "rcp",
